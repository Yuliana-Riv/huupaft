iewModal\" class=\"modal_view\">\r\n           \r\n            <div id=\"modal_add\">\r\n                <div class=\"body dflx\">\r\n                     \r\n                           <h3>Etiqueta agregada</h3>\r\n                           <img src=\"../../assets/add.png\" alt=\"icono alerta\">\r\n                </div>\r\n           \r\n            <div class=\"modal_add_btns dflx\">\r\n                <p @click=\"Add()\" class=\"otro\">Aceptar</p> <p  @click=\"toEdit()\" class=\"editar\">Editar</p>  <p  @click=\"back()\" class=\"inicio\">Inicio</p>\r\n            </div>\r\n          </div>\r\n        </div> \r\n\r\n        </div>\r\n           \r\n    </div>\r\n</template>\r\n<script>\r\nimport {mapActions} from 'vuex';\r\nexport default {\r\n    name:'AddTg',\r\n    data(){\r\n        return{\r\n           status:'',\r\n           message:'',\r\n           name:\"\" ,\r\n        \r\n      active: false,\r\n\r\n\r\n           //MODAL\r\n\r\n            viewModal:false,\r\n\r\n\r\n\r\n        }\r\n    },\r\n  \r\n  \r\n    methods:{\r\n       \r\n         ...mapActions('tag', ['setAddedTg']),\r\n         ...mapActions('tag', ['setHistoryOptionTg']),\r\n          wait: function(){\r\n            setTimeout(() => this.$router.go(), 200);\r\n        },\r\n        submit: async function( name ){\r\n                 this.status =''\r\n                 this.message =''\r\n           let data ={\r\n                    \"name\":name,\r\n             \r\n                  }\r\n\r\n                  \r\n\r\n                    let result = await this.$store.dispatch(\"tag/addItemTg\",  {option:'tag', item: data});\r\n                   \r\n                    if(result.status=='error'){\r\n                        this.status='error'\r\n                        this.message= result.message\r\n                    }else{ // success\r\n                         this.setAddedTg(result.added )\r\n                       this.showModal()\r\n                        \r\n                       \r\n                    } \r\n\r\n            \r\n    },\r\n  closeSelect() {\r\n      this.active = false;\r\n      document.getElementById(\"select-ch\").checked = false;\r\n    },\r\n\r\n     showModal: function(){\r\n         this.viewModal=true\r\n     },\r\n    closeModal:function(){\r\n        this.viewModal=false;\r\n        this.setAddedTg('')\r\n        this.setHistoryOptionTg('Default')\r\n    },\r\n    Add:function(){\r\n        this.status='';\r\n           this.message='';\r\n           this.name=\"\" ;\r\n         \r\n         \r\n        this.viewModal=false;\r\n        this.setAddedTg('');\r\n    },\r\n    toEdit:function(){\r\n        \r\n        \r\n        this.setHistoryOptionTg('Edit')\r\n         this.viewModal=false;\r\n    },\r\n    back:function(){\r\n        setTimeout(() => this.$router.go(), 200);\r\n    }\r\n    \r\n\r\n\r\n   }\r\n}\r\n</script>\r\n<style scoped>\r\n\r\n\r\n    \r\n    #AddTg label{\r\n        color: var(--color-2);\r\n        font-weight: 700;\r\n        font-size: 0.7291666666666666VW;\r\n        margin-bottom: .3vw;\r\n    }\r\n\r\n    #AddTg input, #AddTg select{\r\n        height: 2.5VW;\r\n        width: 14.716666666666668VW;\r\n        border: 0.052083vw solid var(--color-2);\r\n        border-radius: 0vw  1.5vw 1.5vw  0vw ;\r\n        color: var(--color-2);\r\n        font-size: 0.7291666666666666VW;\r\n        font-weight: 500;\r\n        padding-left: 1.09375VW;\r\n    }\r\n\r\n\r\n#select-role {\r\n  height: 2.5vw;\r\n  width: 13.716667vw;\r\n  border: 0.052083vw solid var(--color-2);\r\n  border-radius: 0vw 1.5vw 1.5vw 0vw;\r\n  color: var(--color-2);\r\n  font-size: 0.7291666666666666vw;\r\n  font-weight: 500;\r\n  padding-left: 1.09375vw;\r\n  display: flex;\r\n  align-items: center;\r\n  background-image: url(\"../../assets/blueflecha.svg\");\r\n  background-repeat: no-repeat;\r\n  background-position-y: center;\r\n  background-position-x: 12.6vw;\r\n}\r\n#select-role.rounded {\r\n  border-radi