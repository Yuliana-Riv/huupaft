     };\n      let response = '';\n      try {\n        response = await this.$store.dispatch(\"main/getStatusImage\", payload);\n        if (!response?.data?.status) {\n          this.statusimg = true;\n        } else {\n          response = '';\n        }\n      } catch (err) {\n        console.log(err);\n      }\n      return response;\n    },\n    srcImg: function (img) {\n      if (!img || img == '') {\n        this.statusimg = false;\n        return '';\n      }\n      let src = `${url}/${endpointCodes.get}/personalize-img/${img}`;\n      return src;\n    }\n  },\n  filters: {\n    splitUp: value => {\n      if (!value) return \"\";\n      value = value.toString();\n      let splitStr = value.toUpperCase().split(\" \");\n      return splitStr[0].charAt(0) + splitStr[0].substring(1);\n    }\n  }\n};","map":{"version":3,"mappings":"AAsKA;AAEA;AACA;EACAA;EACAC;IACA;MACAC;MACAC;MAGAC;MACAC;IAEA;EACA;EACA;IACA;IAGA;IAEA;IAEA;IAEA;MACA;MACA;MAGA;QACA;MACA;IACA;EAKA;EACAC;IACAC;MACA;IACA;IACAC;MACA;MAEA;QAEA;UACA;QACA;UACA;YAAAA;UAAA;QACA;MAGA;QACA;UAAAA;QAAA;MACA;IAGA;EACA;EACAC;IACA;IAEA;IAEA;IACA;IAEA;IACA;IAEA;IACA;IAEA;IACA;IAEA;IACA;IAEA;IACA;IAEA;IACA;IAEA;IACA;IAEA;IACA;IAIAC;MACA;QACA;MACA;QACA;MACA;IACA;IACAC;MACA;MACA;IACA;IAEAC;MACA;QACA;MACA;QACA;MAEA;QACA;QACAC;QACA;QAEA;UACA;QACA;UACA;QACA;UACA;QACA;UACA;QACA;UACA;QACA;UACA;QACA;UACA;QACA;UACA;QACA;UACA;QACA;MACA;IACA;IAGAC;MACA;QACA;QACA;MACA;MACA;QACAC;QACAC;MACA;MACA;MAEA;QACAC;QAEA;UACA;QACA;UACAA;QACA;MACA;QACAC;MACA;MAEA;IAEA;IAEAC;MACA;QACA;QACA;MACA;MACA;MAGA;IACA;EAIA;EACAC;IACAC;MACA;MACAC;MAEA;MAEA;IACA;EACA;AACA","names":["name","data","navOption","hidemenu","url","statusimg","computed","identity","logo","methods","activeMenu","getIdentity","setOption","localStorage","status_img","option","image","response","console","srcImg","filters","splitUp","value"],"sourceRoot":"src/components","sources":["SideMenu.vue"],"sourcesContent":["<template>\r\n  <div id=\"SideMenu\" class=\"dflx\">\r\n    <d