{"ast":null,"code":"import { mapActions } from 'vuex';\nexport default {\n  name: 'AddTg',\n  data() {\n    return {\n      status: '',\n      message: '',\n      name: \"\",\n      active: false,\n      //MODAL\n\n      viewModal: false\n    };\n  },\n  methods: {\n    ...mapActions('tag', ['setAddedTg']),\n    ...mapActions('tag', ['setHistoryOptionTg']),\n    wait: function () {\n      setTimeout(() => this.$router.go(), 200);\n    },\n    submit: async function (name) {\n      this.status = '';\n      this.message = '';\n      let data = {\n        \"name\": name\n      };\n      let result = await this.$store.dispatch(\"tag/addItemTg\", {\n        option: 'tag',\n        item: data\n      });\n      if (result.status == 'error') {\n        this.status = 'error';\n        this.message = result.message;\n      } else {\n        // success\n        this.setAddedTg(result.added);\n        this.showModal();\n      }\n    },\n    closeSelect() {\n      this.active = false;\n      document.getElementById(\"select-ch\").checked = false;\n    },\n    showModal: function () {\n      this.viewModal = true;\n    },\n    closeModal: function () {\n      this.viewModal = false;\n      this.setAddedTg('');\n      this.setHistoryOptionTg('Default');\n    },\n    Add: function () {\n      this.status = '';\n      this.message = '';\n      this.name = \"\";\n      this.viewModal = false;\n      this.setAddedTg('');\n    },\n    toEdit: function () {\n      this.setHistoryOptionTg('Edit');\n      this.viewModal = false;\n    },\n    back: function () {\n      setTimeout(() => this.$router.go(), 200);\n    }\n  }\n};","map":{"version":3,"mappings":"AAoEA;AACA;EACAA;EACAC;IACA;MACAC;MACAC;MACAH;MAEAI;MAGA;;MAEAC;IAIA;EACA;EAGAC;IAEA;IACA;IACAC;MACAC;IACA;IACAC;MACA;MACA;MACA;QACA;MAEA;MAIA;QAAAC;QAAAC;MAAA;MAEA;QACA;QACA;MACA;QAAA;QACA;QACA;MAGA;IAGA;IACAC;MACA;MACAC;IACA;IAEAC;MACA;IACA;IACAC;MACA;MACA;MACA;IACA;IACAC;MACA;MACA;MACA;MAGA;MACA;IACA;IACAC;MAGA;MACA;IACA;IACAC;MACAV;